Ti.UI.setBackgroundColor("#fffff");

//Background
var mainWindow = Ti.UI.createWindow({
	backgroundColor: "#c8d2dd",
	backgroundRepeat: "true",
});

//Main view window
var mainView = Ti.UI.createView({
	backgroundColor: "#fff",
	borderColor: "#000"
	height: 200,
	width: 300,
	borderRadius: 4,
	top: 150
	
	
})

mainWindow.open();
